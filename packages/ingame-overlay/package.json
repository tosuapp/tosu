{
  "name": "@tosu/ingame-overlay",
  "version": "4.16.0",
  "description": "High performance tosu ingame overlay",
  "private": true,
  "main": "./dist/src/index.js",
  "scripts": {
    "build": "electron-vite build",
    "dev": "electron-vite dev",
    "watch": "electron-vite dev --watch",
    "dist": "npm run build && electron-builder --windows"
  },
  "keywords": [],
  "author": "storycraft <storycraft@pancake.sh>",
  "dependencies": {
    "@asdf-overlay/core": "^1.0.2",
    "@asdf-overlay/electron": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "electron": "^37.3.1",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.0"
  },
  "build": {
    "productName": "tosu-ingame-overlay",
    "asar": true,
    "files": [
      "dist"
    ],
    "electronLanguages": "en-US",
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "./assets/tosu.ico"
    },
    "directories": {
      "output": "./pack/",
      "app": "."
    },
    "publish": {
      "provider": "github"
    }
  },
  "os": [
    "win32"
  ]
}
