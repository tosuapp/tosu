{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Preserve",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "allowArbitraryExtensions": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "noEmit": true,
  }
}