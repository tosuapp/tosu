{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "DOM",
      "ES2023"
    ],
    "module": "preserve",
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "isolatedModules": true,
    "types": [
      "vite/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@assets/": [
        "assets/*"
      ]
    }
  }
}