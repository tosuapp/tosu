{
    "compilerOptions": {
        "lib": [
            "ES2023"
        ],
        "module": "commonjs",
        "moduleResolution": "Node",
        "allowJs": true,
        "esModuleInterop": true,
        "outDir": "dist",
        "rootDir": "./",
        "sourceMap": false,
        "declaration": false,
        "strict": true,
        "noImplicitAny": false,
        "target": "ES2020",
        "strictPropertyInitialization": false,
        "baseUrl": ".",
    },
    "exclude": [
        "node_modules",
        "dist",
    ],
    "include": [
        "**/*"
    ],
}