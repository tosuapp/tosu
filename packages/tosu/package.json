{
    "name": "tosu",
    "version": "2.4.1",
    "main": "dist/index.js",
    "bin": "dist/index.js",
    "scripts": {
        "genver": "npx genversion _version.js",
        "ts:run": "cross-env NODE_ENV=development ts-node --transpile-only -r tsconfig-paths/register --project tsconfig.json",
        "ts:compile": "ncc build src/index.ts -o dist -m -d",
        "run:dev": "pnpm run genver && pnpm run ts:run src/index.ts",
        "compile:prepare-htmls": "cp -rf node_modules/@tosu/server/assets ./dist",
        "compile": "pnpm run genver && pnpm run ts:compile && pnpm run compile:prepare-htmls && pkg -t node18-win-x64 --output dist/tosu.exe --debug --config pkg.json --compress brotli dist/index.js --options max-old-space-size=512 && pnpm run ts:run src/postBuild.ts"
    },
    "dependencies": {
        "@kotrikd/rosu-pp": "^0.10.0",
        "@tosu/common": "workspace:*",
        "@tosu/game-overlay": "workspace:*",
        "@tosu/server": "workspace:*",
        "@tosu/updater": "workspace:*",
        "osu-catch-stable": "^4.0.0",
        "osu-classes": "^3.0.0",
        "osu-mania-stable": "^5.0.0",
        "osu-parsers": "^4.1.0",
        "osu-standard-stable": "^5.0.0",
        "osu-taiko-stable": "^5.0.0",
        "resedit": "^2.0.0",
        "semver": "^7.5.4",
        "tsprocess": "workspace:*"
    },
    "devDependencies": {
        "@vercel/ncc": "^0.38.1",
        "cross-env": "^7.0.3",
        "genversion": "^3.1.1",
        "pkg": "^5.8.0",
        "ts-node": "^10.9.1"
    }
}
